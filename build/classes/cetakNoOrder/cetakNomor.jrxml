	<queryString>
		<![CDATA[SELECT DISTINCT orders.ID_orders, orders.nama_pemesan, detail_transaksi.total_kue, detail_produk.ukuran,
detail_transaksi.harga_total, produk.nama_produk, detail_produk.nama_produk AS detailKue, 
DATE_FORMAT(tgl_pesan, '%d-%m-%Y') AS tanggalPesan, DATE_FORMAT(tgl_ambil, '%d-%m-%Y') AS tanggalAmbil
FROM transaksi, detail_transaksi, detail_produk, orders, produk, pegawai 
WHERE transaksi.ID_transaksi = detail_transaksi.ID_transaksi 
AND orders.ID_orders = transaksi.ID_orders 
AND detail_produk.kode_sub_produk = detail_transaksi.kode_sub_produk 
AND produk.kode_produk = detail_transaksi.kode_produk 
AND orders.ID_orders = $P{IdOrder};]]>
	</queryString>
